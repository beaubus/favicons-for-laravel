{
    "name": "beaubus/favicons-for-laravel",
    "description": "Choose favicon for you project from a set of 70+ BEAUBUS Favicons",
    "license": "MIT",
    "keywords": [
        "favicon",
        "laravel"
    ],
    "authors": [
        {
            "name": "Daily Web Dev",
            "homepage": "https://www.beaubus.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/beaubus/favicons-for-laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Beaubus\\FaviconsForLaravel\\": "src/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-zip": "*",
        "illuminate/contracts": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Beaubus\\FaviconsForLaravel\\BeaubusFaviconsServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "echo 'installing favicons'",
            "@php artisan vendor:publish --tag=beaubus-favicons --force"
        ]
   }
}
